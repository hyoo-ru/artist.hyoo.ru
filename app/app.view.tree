$hyoo_artist_app $mol_page
	title @ \Artificial Artist
	query? \
	Title <= Query $mol_search
		hint @ \a kitten has cauched the dragon
		query? <=> query_changed? \
		submit? <=> imagine? null
		suggests <= suggests /
	tools /
		<= Index $mol_paginator
			value? <=> index? 0
			backward? => backward?
			forward? => forward?
		<= Lights $mol_lights_toggle
		<= Source $mol_link_source uri \https://github.com/hyoo-ru/artist.hyoo.ru
	body /
		<= Image $mol_image
			uri <= image \
	plugins /
		<= Theme $mol_theme_auto
		<= Hotkey $mol_hotkey key *
			pageUp? <=> backward?
			pageDown? <=> forward?
